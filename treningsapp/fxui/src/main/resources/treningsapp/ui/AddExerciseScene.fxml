<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="607.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="treningsapp.ui.AddExerciseSceneController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="560.0" prefWidth="1218.0" style="-fx-background-color: #007BFF;" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="addExerciseButton" layoutX="64.0" layoutY="119.0" mnemonicParsing="false" onAction="#handleAddExercise" prefHeight="37.0" prefWidth="151.0" text="Add excercise">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" prefHeight="19.0" prefWidth="52.0" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="64.0" layoutY="25.0" prefHeight="28.0" prefWidth="151.0" text="Add exercises">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <TextField fx:id="exerciseNameTextField" layoutX="66.0" layoutY="74.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="63.0" layoutY="225.0" prefHeight="28.0" prefWidth="151.0" text="Current exercises">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <ScrollPane fx:id="exerciseScrollPane" layoutX="15.0" layoutY="260.0" prefHeight="208.0" prefWidth="258.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="238.0" />
              </content>
            </ScrollPane>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="7.0" layoutY="73.0" prefHeight="28.0" prefWidth="64.0" text="Name:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Button fx:id="startWorkoutButton" layoutX="74.0" layoutY="503.0" mnemonicParsing="false" onAction="#handleStartWorkout" prefHeight="37.0" prefWidth="129.0" text="Start Workout">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="removeExerciseButton" layoutX="63.0" layoutY="168.0" mnemonicParsing="false" onAction="#handleRemoveExercise" prefHeight="37.0" prefWidth="151.0" text="Remove Exercise">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
