<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="607.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="treningsapp.ui.FindWorkoutSceneController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="560.0" prefWidth="1218.0" style="-fx-background-color: #007BFF;" VBox.vgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" layoutX="44.0" layoutY="39.0" prefHeight="32.0" prefWidth="191.0" text="Find Workouts" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutY="1.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="19.0" prefWidth="52.0" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <TextField fx:id="searchField" alignment="CENTER" layoutX="65.0" layoutY="97.0" onInputMethodTextChanged="#showWorkouts" onKeyTyped="#showWorkouts" promptText="Search for workout" />
            <ScrollPane fx:id="workoutScrollPane" layoutX="26.0" layoutY="149.0" prefHeight="331.0" prefWidth="229.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
              </content>
            </ScrollPane>
            <Button fx:id="useWorkoutButton" layoutX="98.0" layoutY="533.0" mnemonicParsing="false" onAction="#useWorkout" text="Use Workout" visible="false" />
         </children>
    </AnchorPane>
  </children>
</VBox>
