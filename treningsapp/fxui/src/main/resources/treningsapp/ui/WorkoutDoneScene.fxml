<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="607.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="treningsapp.ui.WorkoutDoneSceneController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="560.0" prefWidth="1218.0" style="-fx-background-color: #007BFF;" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" prefHeight="19.0" prefWidth="52.0" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label alignment="CENTER" layoutX="15.0" layoutY="54.0" prefHeight="37.0" prefWidth="250.0" text="WORKOUT DONE!">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <ScrollPane fx:id="exerciseScrollPane" layoutX="20.0" layoutY="181.0" prefHeight="277.0" prefWidth="239.0">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="275.0" prefWidth="237.0" />
               </content>
            </ScrollPane>
            <Button fx:id="handleBackToMenuButton" layoutX="67.0" layoutY="551.0" mnemonicParsing="false" onAction="#handleBackToMenu" prefHeight="42.0" prefWidth="146.0" text="Back to menu">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label alignment="CENTER" layoutX="45.0" layoutY="134.0" prefHeight="37.0" prefWidth="190.0" text="Workout Recap:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
