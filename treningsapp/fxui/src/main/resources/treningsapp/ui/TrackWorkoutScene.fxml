<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="607.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="treningsapp.ui.TrackWorkoutSceneController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="560.0" prefWidth="1218.0" style="-fx-background-color: #007BFF;" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="useSavedWorkoutButton" layoutX="34.0" layoutY="240.0" mnemonicParsing="false" onAction="#handleUseSavedWorkouts" prefHeight="64.0" prefWidth="213.0" text="Use a saved workout">
               <font>
                  <Font name="Rockwell Extra Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="trackAsYouGoButton" layoutX="34.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleTrackAsYouGo" prefHeight="64.0" prefWidth="213.0" text="Track as you go">
               <font>
                  <Font name="Rockwell Extra Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="19.0" prefWidth="52.0" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
